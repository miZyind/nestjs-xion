  "engines": {
    "node": ">=20",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@1.22.22",
  "dependencies": {
    "@nestjs/common": "^11.0.3",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.3",
    "@nestjs/passport": "^11.0.4",
    "@nestjs/swagger": "^11.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "passport": "^0.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "eslint-plugin-mizyind": "^6.0.10",
    "semantic-release": "^24.2.1",
    "typescript": "^5.7.3"
  },
  "keywords": [
    "nestjs",
    "swagger",
    "guarder",
    "decorator",
    "interceptor",
    "typescript"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
