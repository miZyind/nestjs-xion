{
  "name": "nestjs-xion",
  "version": "0.0.0-development",
  "description": "Excogitative integration of NestJS",
  "repository": "git@github.com:miZyind/nestjs-xion.git",
  "author": "miZyind <mizyind@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/miZyind/nestjs-xion#readme",
  "bugs": "https://github.com/miZyind/nestjs-xion/issues",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --max-warnings 0",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/swagger": "^7.1.13",
    "change-case": "^5.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "passport": "^0.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "0.3.17"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.4",
    "eslint-plugin-mizyind": "^5.0.0",
    "semantic-release": "^22.0.5",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "nestjs",
    "swagger",
    "guarder",
    "decorator",
    "interceptor",
    "typescript"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:mizyind/base"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "lib"
        }
      ],
      "@semantic-release/github"
    ]
  }
