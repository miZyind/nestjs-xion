{
  "name": "nestjs-xion",
  "version": "0.0.0-development",
  "description": "Excogitative integration of NestJS",
  "repository": "git@github.com:miZyind/nestjs-xion.git",
  "author": "miZyind <mizyind@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/miZyind/nestjs-xion#readme",
  "bugs": "https://github.com/miZyind/nestjs-xion/issues",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --max-warnings 0",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/swagger": "^6.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "passport": "^0.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.15"
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "eslint-plugin-mizyind": "^4.10.0",
    "semantic-release": "^21.0.1",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "nestjs",
    "swagger",
    "guarder",
    "decorator",
    "interceptor",
    "typescript"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:mizyind/base"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "lib"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
